! Window opacity
! Opacity was removed in favor of ARGB
! rofi.opacity: 90

! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #88eeeeee,  #5c5dad,  #88ffffff,  #88c9c925,  #000000
rofi.color-urgent: #88eeeeee,  #ad3737,  #88ffffff,  #88ffffff,  #fdf6e3
